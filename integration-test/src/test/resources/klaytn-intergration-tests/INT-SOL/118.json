{
  "tcID":"INT-SOL-118",
  "tcName":"ERC721.setApprovalForAll.false.isApprovedForAll",
  "deploy":{
    "ERC721Mock":{
      "file":"contracts/mocks/ERC721Mock.sol",
      "constructorParams":[]
    }
  },
  "test":[
    {
      "tx":{
        "from":"env.sender",
        "type":"LEGACY",
        "to":"randomMinter",
        "value":"1000000000000000000",
        "gas":100000,
        "data":""
      },
      "expected":{
        "status":true
      }
    },
    {
      "tx":{
        "from":"env.sender",
        "type":"LEGACY",
        "to":"randomOperator",
        "value":"1000000000000000000",
        "gas":100000,
        "data":""
      },
      "expected":{
        "status":true
      }
    },
    {
      "tx":{
        "from":"env.sender",
        "type":"LEGACY",
        "to":"randomOwner",
        "value":"1000000000000000000",
        "gas":100000,
        "data":""
      },
      "expected":{
        "status":true
      }
    },
    {
      "send":{
        "from":"randomMinter",
        "to":"ERC721Mock",
        "gas":100000,
        "method":"mint",
        "params":["randomOwner", "1"]
      },
      "expected":{
        "status":true
      }
    },
    {
      "send":{
        "from":"randomMinter",
        "to":"ERC721Mock",
        "gas":100000,
        "method":"mint",
        "params":["randomOwner", "2"]
      },
      "expected":{
        "status":true
      }
    },
    {
      "send":{
        "from":"randomOwner",
        "to":"ERC721Mock",
        "gas":100000,
        "method":"setApprovalForAll",
        "params":["randomOperator", true]
      },
      "expected":{
        "status":true
      }
    },
    {
      "send":{
        "from":"randomOwner",
        "to":"ERC721Mock",
        "gas":100000,
        "method":"setApprovalForAll",
        "params":["randomOperator", false]
      },
      "expected":{
        "status":true
      }
    },
    {
      "call":{
        "from":"env.sender",
        "to":"ERC721Mock",
        "method":"isApprovedForAll",
        "params":["randomOwner", "randomOperator"]
      },
      "expected":{
        "status":true,
        "returns":false
      }
    }
  ]
}

