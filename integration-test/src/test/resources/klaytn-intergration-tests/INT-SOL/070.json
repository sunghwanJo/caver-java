{
  "tcID":"INT-SOL-070",
  "tcName":"ERC721.safeTransferFrom.approved",
  "deploy":{
    "ERC721Mock":{
      "file":"contracts/mocks/ERC721Mock.sol",
      "constructorParams":[]
    }
  },
  "test":[
    {
      "tx":{
        "from":"env.sender",
        "type":"LEGACY",
        "to":"randomMinter",
        "value":"1000000000000000000",
        "gas":100000,
        "data":""
      },
      "expected":{
        "status":true
      }
    },
    {
      "tx":{
        "from":"env.sender",
        "type":"LEGACY",
        "to":"randomApproved",
        "value":"1000000000000000000",
        "gas":100000,
        "data":""
      },
      "expected":{
        "status":true
      }
    },
    {
      "tx":{
        "from":"env.sender",
        "type":"LEGACY",
        "to":"randomOwner",
        "value":"1000000000000000000",
        "gas":100000,
        "data":""
      },
      "expected":{
        "status":true
      }
    },
    {
      "send":{
        "from":"randomMinter",
        "to":"ERC721Mock",
        "gas":100000,
        "method":"mint",
        "params":["randomOwner", "1"]
      },
      "expected":{
        "status":true
      }
    },
    {
      "send":{
        "from":"randomMinter",
        "to":"ERC721Mock",
        "gas":100000,
        "method":"mint",
        "params":["randomOwner", "2"]
      },
      "expected":{
        "status":true
      }
    },
    {
      "send":{
        "from":"randomOwner",
        "to":"ERC721Mock",
        "gas":100000,
        "method":"approve",
        "params":["randomApproved", "1"]
      },
      "expected":{
        "status":true
      }
    },
    {
      "send":{
        "from":"randomOwner",
        "to":"ERC721Mock",
        "gas":100000,
        "method":"setApprovalForAll",
        "params":["randomOperator", true]
      },
      "expected":{
        "status":true
      }
    },
    {
      "send":{
        "from":"randomApproved",
        "to":"ERC721Mock",
        "gas":100000,
        "method":"safeTransferFrom(address,address,uint256,bytes)",
        "params":["randomOwner", "randomOther", "1", "0x42"]
      },
      "expected":{
        "status":true,
        "events":{
          "Transfer":{
            "from":"randomOwner",
            "to":"randomOther",
            "tokenId":"1"
          }
        }
      }
    },
    {
      "call":{
        "from":"env.sender",
        "to":"ERC721Mock",
        "method":"ownerOf",
        "params":["1"]
      },
      "expected":{
        "status":true,
        "returns":"randomOther"
      }
    },
    {
      "call":{
        "from":"env.sender",
        "to":"ERC721Mock",
        "method":"getApproved",
        "params":["1"]
      },
      "expected":{
        "status":true,
        "returns":"0x0000000000000000000000000000000000000000"
      }
    },
    {
      "call":{
        "from":"env.sender",
        "to":"ERC721Mock",
        "method":"balanceOf",
        "params":["randomOwner"]
      },
      "expected":{
        "status":true,
        "returns":"1"
      }
    }
  ]
}

